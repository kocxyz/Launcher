{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "src/renderer/src/env.d.ts",
    "src/renderer/src/**/*",
    "src/renderer/src/**/*.html",
    "src/renderer/src/**/*.tsx",
    "src/preload/*.d.ts",
  ],
  "compilerOptions": {
    "target": "ESNext",
    "composite": true,
    "jsx": "react-jsx",
    "paths": {
      "@renderer/*": [
        "/src/renderer/src/*"
      ]
    }
  }
}
