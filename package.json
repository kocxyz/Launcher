{
  "name": "knockoutcitylauncher",
  "description": "Unofficial Knockout City Launcher",
  "version": "1.0.0",
  "private": true,
  "homepage": "./",
  "main": "public/electron.js",
  "author": "IPGsystems",
  "dependencies": {
    "@electron/remote": "^2.0.9",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.10.6",
    "@fontsource/roboto": "^5.0.2",
    "@mui/icons-material": "^5.14.8",
    "@mui/material": "^5.13.3",
    "axios": "^1.4.0",
    "discord-rpc": "^4.0.1",
    "moment": "^2.29.4",
    "node-pty": "^0.10.1",
    "react": "^18.2.0", 
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "sudo-prompt": "^9.2.1",
    "tree-kill": "^1.2.2",
    "unzipper": "^0.10.14"
  },
  "devDependencies": {
    "electron": "latest"
  },
  "scripts": {
    "react:dev": "react-scripts start",
    "electron:dev": "electron .",
    "electron:build": "react-scripts build && electron-builder -w -c.extraMetadata.main=build/electron.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "net.ipgg.knockoutcitylauncher",
    "productName": "Knockout City Launcher",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "win": {
      "icon": "/public/icon.png"
    },
    "nsis": {
      "installerLanguages": "en_US"
    }
  }
}
